{
  "author": "Christian Mund <opensource@mundpropaganda.net>",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon_16.png",
      "24": "img/icon_24.png",
      "32": "img/icon_32.png",
      "48": "img/icon_48.png",
      "64": "img/icon_64.png",
      "128": "img/icon_128.png",
      "256": "img/icon_256.png"
    }
  },
  "description": "Chromium browser extension that provides browser and extensions update notifications and extensions management functionality. Chromium versions are synced with Woolyss' Website.",
  "icons": {
    "16": "img/icon_16.png",
    "24": "img/icon_24.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png",
    "256": "img/icon_256.png"
  },
  "homepage_url": "https://github.com/kkkrist/chromium-notifier",
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "name": "Chromium Update Notifications",
  "permissions": ["alarms", "management", "storage", "*://clients2.google.com/service/*"],
  "update_url": "https://raw.githubusercontent.com/kkkrist/chromium-notifier/master/gupdate.xml",
  "version": "2.0.1",
  "version_name": "2.0.1"
}
